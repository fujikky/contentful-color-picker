{
  "name": "color-extension",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@contentful/contentful-extension-scripts": "0.19.1",
    "@contentful/eslint-config-extension": "0.3.1",
    "@babel/core": "7.12.3",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@testing-library/react": "11.1.0",
    "cssnano": "4.1.10",
    "contentful-cli": "1.4.42",
    "eslint": "7.11.0",
    "typescript": "4.0.3",
    "@types/jest": "26.0.15",
    "@types/react": "16.8.6",
    "@types/react-dom": "16.8.4",
    "@types/webpack-env": "1.15.3"
  },
  "scripts": {
    "start": "contentful-extension-scripts start",
    "build": "contentful-extension-scripts build",
    "lint": "eslint ./ --ext .js,.jsx,.ts,.tsx && tsc -p ./ --noEmit",
    "test": "contentful-extension-scripts test --env=jsdom",
    "test:watch": "contentful-extension-scripts test --env=jsdom --watch",
    "test:coverage": "contentful-extension-scripts test --env=jsdom --coverage",
    "deploy": "npm run build && contentful extension update --force",
    "configure": "contentful space use && contentful space environment use",
    "login": "contentful login",
    "logout": "contentful logout",
    "help": "contentful-extension-scripts help"
  },
  "dependencies": {
    "@contentful/forma-36-fcss": "0.2.11",
    "@contentful/forma-36-react-components": "3.62.1",
    "@contentful/forma-36-tokens": "0.9.1",
    "contentful-ui-extensions-sdk": "3.23.2",
    "prop-types": "15.7.2",
    "react": "17.0.0",
    "react-dom": "17.0.0"
  },
  "browserslist": [
    "last 5 Chrome version",
    "> 1%",
    "not ie <= 11"
  ]
}
